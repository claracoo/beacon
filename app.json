{
  "expo": {
    "name": "Signally",
    "description": "A helper to public signage for the visually impaired",
    "githubUrl": "https://github.com/claracoo/beacon",
    "slug": "signally",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "org.rahulhaque.compass"
    },
    "android": {
      "package": "org.rahulhaque.compass"
    }
  }
}
